{
  "name": "fc-cv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "firebase": "^4.11.0",
    "jquery-lite": "^2.2.3",
    "node-sass-chokidar": "^1.1.2",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --source-map src/Styles/Main/index.scss src/Styles/Main/index.css",
    "watch-css": "nodemon -e scss -x npm-run-all build-css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build npm-run-all build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
